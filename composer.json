{
	"name": "cabag/ext-backend-progress",
	"description": "Add a toolbaritem that displays the progress of a long running task.",
	"type": "typo3-cms-extension",
	"authors": [
		{
			"name": "Tizian Schmidlin",
			"role": "Developer"
		}
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"php": "7.2",
		"typo3/cms-core": "^9.5",
		"typo3/cms-backend": "^9.5",
		"typo3/cms-extbase": "^9.5"
	},
	"require-dev": {
		"phpstan/phpstan": "*",
		"squizlabs/php_codesniffer": "*"
	},
	"autoload": {
		"psr-4": {
			"Cabag\\BackendProgress\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "backend_progress"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse",
		"phpcs": "phpcs",
		"phpcsfix": "phpcbf"
	}
}
